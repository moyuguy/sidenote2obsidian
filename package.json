{
    "name": "sidenote2obsidian-plasmo",
    "displayName": "Sidenote2Obsidian",
    "version": "2.1.0",
    "description": "Quickly record ideas to Obsidian from Chrome Side Panel",
    "author": "Antigravity",
    "scripts": {
        "dev": "plasmo dev",
        "build": "plasmo build",
        "package": "plasmo package"
    },
    "dependencies": {
        "@plasmohq/storage": "^1.11.0",
        "clsx": "^2.1.1",
        "lucide-react": "^0.395.0",
        "marked": "^17.0.1",
        "plasmo": "0.89.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "tailwind-merge": "^2.3.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.19",
        "@types/chrome": "0.0.268",
        "@types/node": "20.14.2",
        "@types/react": "18.3.3",
        "@types/react-dom": "18.3.0",
        "autoprefixer": "^10.4.19",
        "postcss": "^8.4.38",
        "tailwindcss": "^3.4.4",
        "typescript": "5.4.5"
    },
    "manifest": {
        "host_permissions": [
            "<all_urls>"
        ],
        "permissions": [
            "sidePanel",
            "storage",
            "tabs"
        ],
        "commands": {
            "_execute_action": {
                "suggested_key": {
                    "default": "Ctrl+Shift+O",
                    "mac": "MacCtrl+Shift+O"
                },
                "description": "Toggle Side Panel"
            }
        },
        "default_locale": "en"
    }
}